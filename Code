        "from selenium.webdriver.common.by import By\n",
        "from selenium.webdriver.support import expected_conditions as EC\n",
        "from selenium.webdriver.support.ui import WebDriverWait\n",
        "\n",
        "# Open the webpage\n",
        "int_list = [int(s.replace(',', '')) for s in second_select_options]\n",
        "print(int_list)\n",
        "print(len(int_list))\n",
        "\n",
        "# Remove commas and convert to integers\n",
        "print(first_select_options)\n",
        "#first_select_options = [int(num.replace(',', '')) if not num.replace(',', '').isdigit() else int(num) for num in first_select_options]\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "def extract_elements_data(x):\n",
        "  final_list = []\n",
        "  for item in first_select_options[:-1]:\n",
        "    element = driver.get(\"https://www.klaviyo.com/pricing?contacts=\"+str(item)+\"&smsCredits=\"+str(x))\n",
        "    target_element = driver.find_element(By.XPATH, \"./html/body/div[1]/div[1]/div/main/div/div[2]/div[2]/div[2]/div/div[2]/div[1]\")\n",
        "    ele = target_element.text\n",
        "    i = 0\n",
        "    while(ele==\"$60\" and i !=3):\n",
        "      element = driver.get(\"https://www.klaviyo.com/pricing?contacts=\"+str(item)+\"&smsCredits=\"+str(x))\n",
        "      target_element = driver.find_element(By.XPATH, \"./html/body/div[1]/div[1]/div/main/div/div[2]/div[2]/div[2]/div/div[2]/div[1]\")\n",
        "      ele = target_element.text\n",
        "      i = i +1\n",
        "\n",
        "    final_list.append(ele)\n",
        "  print(final_list)\n",
        "  return final_list\n",
        "\n",
        "# print complete element\n",
        "df = pd.DataFrame({'SMS': first_column_list})\n",
