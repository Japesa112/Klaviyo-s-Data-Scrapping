      "cell_type": "code",
      "source": [
        "import requests\n",
        "import json\n",
        "from bs4 import BeautifulSoup\n",
        "import re\n",
        "\n",
        "URL = \"https://www.klaviyo.com/pricing?contacts=501&smsCredits=150\"\n",
        "url = URL\n",
        "response = requests.get(url)\n",
        "html_content = response.text\n",
        "soup = BeautifulSoup(html_content, 'html.parser')\n",
        "# Find the table element\n",
        "select_elements = soup.find_all('select')\n",
        "\n",
        "first_select_options = []\n",
        "second_select_options = []\n",
        "\n",
        "# Loop through each select element\n",
        "for index, select in enumerate(select_elements):\n",
        "    options = select.find_all('option')\n",
        "\n",
        "    # Print out the text value of each option\n",
        "    for option in options:\n",
        "        if index == 0:\n",
        "            first_select_options.append(option.text)\n",
        "        elif index == 1:\n",
        "            second_select_options.append(option.text)\n",
        "\n",
        "# Print the options for the first select element\n",
        "print(\"Options for the first select element:\")\n",
        "print(first_select_options)\n",
        "\n",
        "# Print the options for the second select element\n",
        "print(\"\\nOptions for the second select element:\")\n",
        "print(second_select_options)\n",
        "first_select = soup.find('select')\n",
        "\n",
        "# Extract options from the first select element\n",
        "first_select_options = []\n",
        "SMS = []\n",
        "n = 1\n",
        "for option in first_select.find_all('option'):\n",
        "    # Split each option value by the hyphen and extract the first element\n",
        "    first_element = option.text.split(' - ')[0].strip()\n",
        "\n",
        "    # Append the first element to the list\n",
        "    first_select_options.append(first_element)\n",
        "\n",
        "\n",
        "# Print the first elements of each option in the first select element\n",
        "print(\"First elements of options in the first select element:\")\n",
        "item = '250,001'\n",
        "first_select_options[len(first_select_options)-1] = item\n",
        "print(first_select_options)\n",
        "print(second_select_options)\n",
        "\n",
        "\n"
